{% extends 'base.html.twig' %}
{% block body %}
<div class="container">
    <table class="table">
        <thead>
            <th scope="col">Id</th>
            <th scope="col">Name Surname</th>
            <th scope="col">Work Per Hour</th>
        </thead>
        <tbody>
            {% for employee in employees %}
                <tr>
                    <td>{{ employee.id }}</td>
                    <td>{{ employee.nameSurname }}</td>
                    <td>{{ employee.workPerHour }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}